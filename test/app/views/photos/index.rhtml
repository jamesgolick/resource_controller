<h1>Listing photos</h1>

<table>
  <tr>
    <th>Title</th>
  </tr>
  
<% for photo in @photos %>
  <tr>
    <td><%=h photo.title %></td>
    <td><%= link_to 'Show', photo_path(photo) %></td>
    <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
    <td><%= link_to 'Destroy', photo_path(photo), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New photo', new_photo_path %>